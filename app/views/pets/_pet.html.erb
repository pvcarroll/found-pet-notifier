<% @matches.each do |match| %>
    <p>
      <u>Possible match found</u><br />
      Type: <%= match[:animal_type] %><br />
      Looks like (breed): <%= match[:breed] %><br />
      Color: <%= match[:color] %><br />
      Sex: <%= match[:sex] %><br />
      Found Location: <%= match[:found_location] %><br />
      Link with image: <%= match[:image] %>
    </p>
<% end %>